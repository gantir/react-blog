# DSquare Solutions EditorConfig settings
# http://editorconfig.org/

# use the top most editor config file
root = true

[*]
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true
charset = utf-8
end_of_line = lf

[*.md]
max_line_length = 0
trim_trailing_whitespace = false

[Makefile]
indent_style = tab

[*.clj]
indent_size = 2

[*.css]
indent_size = 2

[*.html]
indent_size = 4

[*.js]
indent_size = 4

[*.jsx]
indent_size = 4
